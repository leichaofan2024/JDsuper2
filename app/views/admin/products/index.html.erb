<div class="col-md-10">
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>#</th>
        <th>title</th>
        <th>description</th>
        <th>price</th>
        <th>quantity</th>
      </tr>
    </thead>
    <tbody>
      <% @products.each do |product| %>
        <tr>
          <td><%= product.id %></td>
          <td><%= link_to(product.title, admin_products_path) %></td>
          <td><%= product.description %></td>
          <td><%= product.price %></td>
          <td><%= product.quantity %></td>
          <td><%= product.created_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
